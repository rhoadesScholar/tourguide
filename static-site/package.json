{
  "name": "neuroglancer-tourguide-static",
  "version": "1.0.0",
  "description": "Static site version of Neuroglancer Tourguide with client-side LLM API integration",
  "scripts": {
    "serve": "python -m http.server 8080",
    "serve-node": "npx http-server -p 8080",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "npm run test && npm run test:e2e",
    "lint": "eslint app.js organelle-data.js --fix",
    "lint:check": "eslint app.js organelle-data.js"
  },
  "keywords": [
    "neuroglancer",
    "electron-microscopy",
    "visualization",
    "ai",
    "static-site"
  ],
  "author": "rhoadesScholar",
  "license": "BSD-3-Clause",
  "dependencies": {},
  "devDependencies": {
    "@playwright/test": "~1.40.0",
    "@types/jest": "~29.5.11",
    "eslint": "~8.56.0",
    "http-server": "~14.1.1",
    "jest": "~29.7.0",
    "jest-environment-jsdom": "~29.7.0"
  }
}
